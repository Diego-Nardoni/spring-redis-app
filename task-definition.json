{
  "family": "spring-redis-app-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::221082174220:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::221082174220:role/spring-redis-app-ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "spring-redis-app-app",
      "image": "221082174220.dkr.ecr.us-east-1.amazonaws.com/spring-redis-app:latest",
      "cpu": 0,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "production"
        },
        {
          "name": "_X_AMZN_TRACE_ID",
          "value": ""
        },
        {
          "name": "AWS_XRAY_TRACING_NAME",
          "value": "SpringRedisApp"
        },
        {
          "name": "AWS_XRAY_CONTEXT_MISSING",
          "value": "LOG_ERROR"
        }
      ],
      "secrets": [
        {
          "name": "SPRING_DATA_REDIS_HOST",
          "valueFrom": "/spring-redis-app/redis/endpoint"
        },
        {
          "name": "SPRING_DATA_REDIS_PORT",
          "valueFrom": "/spring-redis-app/redis/port"
        },
        {
          "name": "SPRING_DATA_REDIS_SSL_ENABLED",
          "valueFrom": "/spring-redis-app/redis/ssl"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/spring-redis-app-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/actuator/health/readiness || exit 1"
        ],
        "interval": 15,
        "timeout": 5,
        "retries": 2,
        "startPeriod": 60
      },
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": []
    }
  ],
  "volumes": [],
  "placementConstraints": []
}
